{
  "name": "baeksang-app-api",
  "version": "1.0.0",
  "description": "API for Backsang Awards",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only ./src/server.js",
    "build": "yarn clean && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "code:format": "prettier --write \"src/**/*.{ts,css,js,html}\"",
    "code:check": "prettier --check \"src/**/*.{ts,css,js,html}\"",
    "code:lint": "eslint \"./**/*.js\" --quiet",
    "seed": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only ./src/seeds/seed.ts",
    "seed2": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only ./src/seeds/seed-notices.js",
    "seed3": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only ./src/seeds/seed-users.js",
    "seed4": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only ./src/seeds/seed-notices_comments.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "baeksang",
    "Express",
    "RestAPI",
    "MongoDB",
    "Mongoose"
  ],
  "author": "Haneul Cho",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "4.17.1",
    "extend": "^3.0.2",
    "jsonwebtoken": "^9.0.1",
    "mkdirp": "^3.0.1",
    "mongoose": "^7.4.0",
    "mongoose-paginate-v2": "^1.7.23",
    "mongoose-sequence": "^5.3.1",
    "multer": "^1.4.5-lts.1",
    "rimraf": "^5.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.4",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
